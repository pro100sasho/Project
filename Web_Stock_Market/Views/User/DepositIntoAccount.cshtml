@model User
@{
    ViewData["Title"] = "DepositIntoAccount";
}

<h1>Deposit Into Account</h1>

<div class="col-md-4">
    <form action="DepositIntoAccount" method="post">
        <label>Amount</label>
        <input class="form-control" type="text" name="amount" />
        <button style="margin-top:8px" class="btn btn-primary" type="submit">Deposit</button>
    </form>
</div>




















